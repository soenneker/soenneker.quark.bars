@inherits Soenneker.Quark.Element

<hr @attributes="BuildAttributes()" />

@code {
	public override string? Name { get; set; } = "BarDropdownDivider";

    protected override Dictionary<string, object> BuildAttributes()
    {
        Dictionary<string, object> attributes = base.BuildAttributes();

        AppendToClassAttr(attributes, "dropdown-divider");

        return attributes;
    }

}
